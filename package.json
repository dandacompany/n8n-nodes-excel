{
  "name": "n8n-nodes-excel",
  "version": "1.0.2",
  "description": "An n8n node to handle local Excel and CSV files like a database, allowing you to create, read, update, and delete data within your workflows.",
  "license": "MIT",
  "author": {
    "name": "dante",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dante/n8n-nodes-excel.git"
  },
  "main": "dist/nodes/Excel/Excel.node.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint nodes --ext .js,.ts",
    "lint:fix": "eslint nodes --ext .js,.ts --fix",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Excel/Excel.node.js"
    ]
  },
  "dependencies": {
    "fast-csv": "^5.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~4.8.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
